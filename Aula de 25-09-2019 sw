enable
configure terminal
hostname SW-CORE-31
banner motd "ACESSO APENAS PARA PESSOAS AUTORIZADAS"
enable secret SenhadaEnable
username Yohan privilege 15 secret Batata01
username Augusto privilege 15 secret Batata01
username Lucas privilege 15 secret Batata01
username Kaiky privilege 15 secret Batata01
ip domain-name peachanddaisy.local
crypto key generate rsa general-keys modulus 1024
line console 0
login local
exec-timeout 10
line vty 0 15
transport input ssh
login local
exec-timeout 10
exit
service password-encryption
interface vlan 100
ip address 172.16.4.197 255.255.255.224
interface vlan 100
ip address 172.16.4.222 255.255.255.224
no shutdown
exit
ip default-gateway 172.16.4.1
interface gigabitethernet 1/1
switchport mode trunk
switchport trunk native vlan 99
switchport trunk allowed vlan 10,20,99,10
description CONEXAO COM SWITCH-31
exit
interface gigabitethernet 2/1
switchport mode trunk
switchport trunk native vlan 99
switchport trunk allowed vlan 10,20,99,10
description CONEXAO COM SWITCH-32
exit
interface gigabitethernet 0/1
switchport mode trunk
switchport trunk native vlan 99
switchport trunk allowed vlan 10,20,99,100
description CONEXAO COM RT-31
exit
interface gigabitethernet 3/1
switchport mode access
switchport access vlan 100
description CONEXAO COM SRV-31
no shutdown
do wr

--------------------------------------------------------

SW-12




enable 
configure terminal
hostname SW-12
banner motd "ACESSO APENAS PARA PESSOAS AUTORIZADAS"
enable secret SenhadaEnable
username Yohan privilege 15 secret Batata01
username Augusto privilege 15 secret Batata01
username Lucas privilege 15 secret Batata01
username Kaiky privilege 15 secret Batata01
ip domain-name peachanddaisy.local
crypto key generate rsa general-keys modulus 1024
line console 0
login local
exec-timeout 10
line vty 0 15
transport input ssh
login local
exec-timeout 10
exit
service password-encryption
ip default-gateway 172.16.1.129
vlan 10
name VLAN10
vlan 20
name VLAN20
vlan 100
name GERENCIAMENTO
interface fastEthernet 0/1
switchport mode access
switchport access vlan 10
description PC-13
interface fastEthernet 0/2
switchport mode access
switchport access vlan 20
description PC-14
interface fastEthernet 0/24
switchport mode access
switchport access vlan 100
description SRV
interface gigabitEthernet 0/1
switchport mode trunk
switchport trunk native vlan 99
switchport trunk allowed vlan 10,20,99,100
description TRUNK ROUTER
interface vlan 10
ip address 172.16.1.11 255.255.255.128
interface vlan 20
ip address 172.16.0.11 255.255.255.0
interface vlan 100
ip address 172.16.1.133 255.255.255.240
interface vlan 100 
ip address 172.16.1.140 255.255.255.240
no shutdown
do wr

